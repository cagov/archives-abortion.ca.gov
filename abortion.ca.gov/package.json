  "scripts": {
    "build": "eleventy",
    "dev": "npm run site:watch:dev",
    "site:build:dev": "cross-env NODE_ENV=development eleventy",
    "site:watch:dev": "cross-env NODE_ENV=development eleventy --serve --incremental",
    "site:watch": "eleventy --serve",
    "start": "npm run site:watch",
    "test": "npx playwright test test/finder.spec.mjs test/e2e.spec.mjs --project=chromium --config test/playwright.e2e.config.js",
    "linkcheck": "npx playwright  test  test/linkcheck.spec.mjs --config=test/playwright.e2e.config.js",
    "preinstall": "git config core.hooksPath git-hooks",
    "test:serve": "npm run build && npx http-server _site/ -p 8081",
    "test:setup": "npx playwright install-deps chromium && npx playwright install"
  },
